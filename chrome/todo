(function() {
	chrome.tabs.query({currentWindow: true, active: true}, function(tabs) {
		// check if URL contains token
		var parts = /access_token=(.+)/.exec(tabs[0].url);
		if (parts) {
			// store it for the future
			var token = parts[1];
			chrome.storage.sync.set({token: token}, function() {
				console.log('INFO - User token in URL found and saved');
			});
		}
		else {
			console.log('ERROR - No user token in URL found');
		}
	});
})();

